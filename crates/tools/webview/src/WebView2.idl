/// This interface is an extension of `ICoreWebView2_14` that supports status Favicons.
[uuid(517B2D1D-7DAE-4A66-A4F4-10352FFB9518), object, pointer_default(unique)]
interface ICoreWebView2_15 : ICoreWebView2_14 {

/// \snippet ScenarioWebViewEventMonitor.cpp HttpRequestHeaderIterator
[uuid(0702fc30-f43b-47bb-ab52-a42cb552ad9f), object, pointer_default(unique)]
interface ICoreWebView2HttpHeadersCollectionIterator : IUnknown {

